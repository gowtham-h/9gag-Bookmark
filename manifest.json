{
  "name": "9GAG Bookmark",
  "version": "0.10.3",
  "manifest_version": 2,
  "description": "Continue 9gag from where you left off",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "9TAG",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "storage",
    "*://*.9gag.com/*"
  ],
  "web_accessible_resources": [
    "icons/*.png"
  ]
}
